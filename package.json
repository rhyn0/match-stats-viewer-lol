{
    "name": "match-stats-viewer-lol",
    "version": "1.0.0",
    "private": true,
    "description": "Fullstack ReactJS application for viewing statistics for a local LoL tournament.",
    "type": "module",
    "scripts": {
        "dev": "vinxi dev",
        "build": "vinxi build",
        "start": "vinxi start",
        "prepare": "husky",
        "lint": "pnpm run lint:biome",
        "lint:types": "tsc --noEmit",
        "lint:biome": "pnpm biome check --error-on-warnings ./",
        "format": "pnpm biome format --write ",
        "pre-commit": "pnpm lint && pnpm test",
        "generate": "drizzle-kit generate",
        "push": "drizzle-kit push",
        "studio": "drizzle-kit studio --port 3001 --verbose",
        "fix": "pnpm run fix:biome",
        "fix:biome": "npx biome check --write ./",
        "test": "vitest run --silent",
        "test:watch": "vitest",
        "plop": "plop"
    },
    "keywords": [],
    "author": "Ryan Ozawa<ryan.ozawa21@gmail.com>",
    "license": "ISC",
    "dependencies": {
        "@libsql/client": "^0.14.0",
        "@radix-ui/react-dialog": "^1.1.4",
        "@radix-ui/react-avatar": "^1.1.2",
        "@radix-ui/react-dropdown-menu": "^2.1.4",
        "@radix-ui/react-navigation-menu": "^1.2.3",
        "@radix-ui/react-scroll-area": "^1.2.2",
        "@radix-ui/react-slot": "^1.1.1",
        "@radix-ui/react-tooltip": "^1.1.6",
        "@tailwindcss/vite": "4.0.0-beta.9",
        "@tanstack/react-query": "^5.64.1",
        "@tanstack/react-query-devtools": "^5.64.1",
        "@tanstack/react-router": "1.97.1",
        "@tanstack/react-router-with-query": "^1.97.1",
        "@tanstack/router-devtools": "^1.97.1",
        "@tanstack/start": "1.97.2",
        "arktype": "^2.0.0",
        "class-variance-authority": "0.7.1",
        "clsx": "2.1.1",
        "dotenv": "^16.4.7",
        "drizzle-orm": "^0.38.4",
        "lucide-react": "0.473.0",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "recharts": "^2.15.0",
        "tailwind-merge": "2.6.0",
        "tailwindcss": "4.0.0-beta.9",
        "tailwindcss-animate": "1.0.7",
        "vinxi": "0.5.1"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.2.0",
        "@types/node": "^22.10.7",
        "@types/react": "^19.0.7",
        "@types/react-dom": "^19.0.3",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitest/coverage-istanbul": "^3.0.2",
        "drizzle-kit": "^0.30.2",
        "drizzle-seed": "^0.3.0",
        "husky": "^9.1.7",
        "jsdom": "^26.0.0",
        "lint-staged": "^15.4.1",
        "plop": "^4.0.1",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.0.2"
    },
    "overrides": {
        "react-is": "^19.0.0-rc-69d4b800-20241021"
    },
    "lint-staged": {
        "*.+(ts|tsx)": ["pnpm run lint", "bash -c 'pnpm run lint:types'"],
        "*.test.+(ts|tsx)": ["pnpm test"],
        "*.json": "pnpm run format"
    },
    "pnpm": {
        "onlyBuiltDependencies": ["@biomejs/biome"]
    }
}
